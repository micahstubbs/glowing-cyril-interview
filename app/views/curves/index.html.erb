<h1>Listing curves</h1>

<table>
  <tr>
    <th>Market</th>
    <th>Commodity</th>
    <th>ID</th>
    <th></th>
    <th></th>
  </tr>

<% @curves.each do |curve| %>
  <tr>
    <td><%= curve.market %></td>
    <td><%= curve.commodity %></td>
    <td><%= curve.id %></td>
  </tr>
<% end %>
</table>

<br />


<h2>Import Curves</h2>
<%= form_tag import_curves_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>