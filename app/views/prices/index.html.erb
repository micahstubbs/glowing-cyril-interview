<h1>Listing prices</h1>

<table>
  <tr>
    <th>Curve</th>
    <th>Month</th>
    <th>Settle</th>
    <th>ID</th>
    <th></th>
    <th></th>
  </tr>

<% @prices.each do |price| %>
  <tr>
    <td><%= price.curve_id %></td>
    <td><%= price.month_id %></td>
    <td><%= price.settle %></td>
    <td><%= price.id %></td>
  </tr>
<% end %>
</table>

<br />


<h2>Import Prices</h2>
<%= form_tag import_prices_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>